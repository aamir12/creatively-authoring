<!doctype html>
<html lang="en">

<head>
  <%- include('../parts/allCSS'); %>
  <script src="author_public/PaperDashBoard/js/jquery-3.1.1.min.js" type="text/javascript"></script>
  
</head>

<body>
  <div class="wrapper">
    <%- include('../parts/leftNavBar'); %>
      <div class="main-panel">
        <%- include('../parts/topNavBarPart1'); %>
          <%- include('../parts/topNavBarPart2'); %>
            <div class="content">
              <div class="container-fluid">
                <div class="row">
                  <div class="col-md-8 col-md-offset-2">
                    <div class="card">
                      <div class="card-content">
                          <%var data = JSON.parse(data)%>

                          <% if(data.result[0].success){ %>
                        <h4>User logout status</h4>
                        <div class="success"><i class="fa fa-thumbs-o-up"></i>
                          <%-  data.result[0].success%>
                        </div>

                        <% }else if(data.result[0].error){%>
                        <h4>User logout status</h4>
                        <div class="error"><i class="fa fa-exclamation-triangle"></i>
                          <%-  data.result[0].error%>
                        </div>
                        <% } else{}%>

                        <br/>

                        <% if(data.result[1].success){ %>
                        <h4>Quiz reset status</h4>
                        <div class="success"><i class="fa fa-thumbs-o-up"></i>
                          <%-  data.result[1].success%>
                        </div>

                        <% }else if(data.result[1].error){%>
                        <div class="error"><i class="fa fa-exclamation-triangle"></i>
                        <h4>Quiz reset status</h4>
                          <%-  data.result[1].error%>
                        </div>
                        <% } else{}%>

                      <br/>
                      <b style="font-size:16px;">You will be redirected to "Quiz Control Board" in 10 seconds. If to want to redirect now <a href="/author_create_quiz">Click Here</a></b>
                      <script>
                          var timer = setTimeout(function() {
                              window.location='/author_create_quiz'
                          }, 10000);
                      </script>


                      </div>
                  </div>
                  </div>
                </div>
              </div>
            </div>
             <%- include('../parts/footer'); %>
      </div>
     
  </div>
  </div>
</body>
<%- include('../parts/allJS'); %>
</html>