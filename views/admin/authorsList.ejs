<!DOCTYPE html>
<html>
  <head>
    <%- include('../parts/allCSS'); %>
    
    <script src="author_public/PaperDashBoard/js/jquery-3.1.1.min.js" type="text/javascript"></script>
    <script type="text/javascript">
      $(document).ready(function() {
        if (window.location.search.indexOf('msg') > -1) {
            $('#summ_notfound').slideDown();
            function success_1(){
              $('#summ_notfound').slideUp();
            }
            setTimeout(success_1, 5000);
        } else {
            // alert('track not here');
        }
      });
    </script>
    <style>
     .standOut{
      font-weight: bold;
      font-size: 18px;
     }
     .ng_pageRow{
         display: flex;
         flex-flow: row;
         flex-wrap: wrap;
         align-items: center;
         justify-content: space-between;
     }
    </style>
  </head>
  
<body  ng-app="manageAuthorlistApp" ng-controller="manageAuthorlistContoller" ng-init="initData()" ng-cloak>
    <div class="wrapper">
        <div class="ld ld-ring ld-spin"></div> 
        <%- include('../parts/leftNavBar'); %>
        <div class="main-panel" style="display:none;"  id="mainPanelDiv">
            <%- include('../parts/topNavBarPart1'); %>Authors List<br/> 
           
            <%- include('../parts/topNavBarPart2'); %>
            <div class="content">
                <div class="container-fluid">
                <div class='row'>
                  <div class='col-md-10 col-md-offset-1'> <!-- col-md-10 col-md-offset-1 -->
                    <div class="card" id="mainCard">
                        <div class="card-content" >

                            <div class="nav-tabs-navigation" id="mainTab">
                                <div class="nav-tabs-wrapper">
                                  <ul id="tabs" class="nav nav-tabs" data-tabs="tabs">
                                      <li class="active">
                                        <a href="#allAuthors" data-toggle="tab">Authors
                                            
                                        </a>
                                      </li>
                                      <li><a href="#newRequest" data-toggle="tab">New Request
                                        <span class="badge label-info ticketCounters" ng-show="data.pending.length>0" >{{data.pending.length}}</span>
                                      </a>
                                      </li>
                                      <li><a href="#blocked" data-toggle="tab">Blocked</a>
                                      </li>
                                      <li><a href="#rejected" data-toggle="tab">Rejected</a>
                                      </li>
                                  </ul>
                                </div>
                            </div>

                              <div id="my-tab-content1" class="tab-content" >
                                <div class="tab-pane active tabCls" id="allAuthors">
                                    <div class="row">
                                        <div class="col-sm-2 col-md-2">
                                            <select ng-model="itemsPerPages_allAuthors" class="form-control ngDT_select"
                                            ng-init="itemsPerPages_allAuthors =  10"
                                            ng-change="resetPage('allAuthors')"
                                            ng-options="o as o for o in [5,10,25,100]">
                                            </select>
                                        </div>
                                        <div class="col-sm-7 col-md-8">
                                        </div>
                                        <div class="col-sm-3 col-md-2 pull-right">
                                           <input type="text"  ng-model="allAuthorsSearch" class="form-control" placeholder="Search" ng-keyup="onSearch('allAuthors')">
                                        </div>
                                    </div>
                                    <div class="table-responsive">
                                            <div ng-show="isLoading_allAuthors">
                                                Loading...
                                            </div>
                                            
                                            <table id="allAuthorsTab" class="table table-no-bordered ngDT" ng-show="!isLoading_allAuthors" >
                                            <thead>
                                                <tr>
                                                <th ng-click="sort('name','allAuthors')">
                                                    Name <i class="fa  ngDT_sortIcon pull-right" ng-show="sortKeyAllAuth=='name'" ng-class="{'fa-caret-up':reverseAllAuth,'fa-caret-down':!reverseAllAuth}"></i>
                                                </th>
                                                <th ng-click="sort('email','allAuthors')">
                                                    Email <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyAllAuth=='email'" ng-class="{'fa-caret-up':reverseAllAuth,'fa-caret-down':!reverseAllAuth}"></i>
                                                </th>
                                                <th ng-click="sort('createdOn','allAuthors')">
                                                    Created On <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyAllAuth=='createdOn'" ng-class="{'fa-caret-up':reverseAllAuth,'fa-caret-down':!reverseAllAuth}"></i>
                                                </th>
                                                <th  class="td-actions text-center">Actions</th>
                                                </tr>
                                            </thead>
                                                    <tbody>
                                                        <tr dir-paginate="users in filter_allAuthors  = (data.active | orderBy:sortKeyAllAuth:reverseAllAuth | filter: allAuthorsSearch  )  | itemsPerPage: itemsPerPages_allAuthors" current-page="currPageAllAuth"  
                                                        pagination-id = "allAuthors"
                                                        my-repeat-directive = "allAuthors">
                                                            <td>{{ users.name }}</td>
                                                            <td>{{users.email}}</td>
                                                            <td>{{users.createdOn  }}</td>
                                                            <td class="text-center">
                                                               
                                                                    <button type="button"  ng-click="viewDetail(users)" class="btn btn-simple btn-warning btn-icon table-action edit"  title="View Detail" data-original-title="View Detail"  rel="tooltip"><i class="ti-eye"  ></i></button>
                                                                
                                                            </td>
                                                        </tr>

                                                        <tr ng-show="filter_allAuthors.length==0">
                                                            <td colspan="4" class="text-center">
                                                                No Data Available
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                            </table>
                                    </div>

                                    <div class="ng_pageRow">
                                       
                                           <div ng-if="filter_allAuthors.length>0">
                                            Showing {{from_allAuthors}} 
                                            to {{  (to_allAuthors>=filter_allAuthors.length) ? filter_allAuthors.length : to_allAuthors }} 
                                            of {{filter_allAuthors.length}}  entries
                                           </div>
                                           
                                            <dir-pagination-controls
                                            max-size="10"
                                            direction-links="true"
                                            boundary-links="true"
                                            pagination-id = "allAuthors">
                                           </dir-pagination-controls>
                                        
                                    </div>
                                    

                                </div>
                             
                                <div class="tab-pane tabCls" id="newRequest">
                                    <div class="row">
                                        <div class="col-sm-2 col-md-2">
                                            <select ng-model="itemsPerPages_newRequest" class="form-control ngDT_select"
                                            ng-init="itemsPerPages_newRequest =  10"
                                            ng-change="resetPage('newRequest')"
                                            ng-options="o as o for o in [5,10,25,100]">
                                            </select>
                                        </div>
                                        <div class="col-sm-7 col-md-8">
                                        </div>
                                        <div class="col-sm-3 col-md-2 pull-right">
                                           <input type="text" ng-model="newRequestSearch" class="form-control" placeholder="Search" ng-keyup="onSearch('newRequest')">
                                        </div>
                                    </div>
                                    <div class="table-responsive">
                                            <div ng-show="isLoading_newRequest">
                                                Loading...
                                            </div>
                                            <table id="newRequestTab" class="table table-no-bordered ngDT" ng-show="!isLoading_allAuthors" >
                                            <thead>
                                                <tr>
                                                    <th ng-click="sort('name','newRequest')">
                                                        Name <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyNewReq=='name'" ng-class="{'fa-caret-up':reverseNewReq,'fa-caret-down':!reverseNewReq}"></i>
                                                    </th>
                                                    <th ng-click="sort('email','newRequest')">
                                                        Email <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyNewReq=='email'" ng-class="{'fa-caret-up':reverseNewReq,'fa-caret-down':!reverseNewReq}"></i>
                                                    </th>
                                                    <th ng-click="sort('createdOn','newRequest')">
                                                        Created On <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyNewReq=='createdOn'" ng-class="{'fa-caret-up':reverseNewReq,'fa-caret-down':!reverseNewReq}"></i>
                                                    </th>
                                                    <th  class="td-actions text-center">Actions</th>
                                                </tr>
                                            </thead>
                                                    <tbody>
                                                        <tr dir-paginate="users in filter_newRequest  = (data.pending | orderBy:sortKeyNewReq:reverseNewReq| filter: newRequestSearch)  | itemsPerPage: itemsPerPages_newRequest" current-page="currPageNewReq"  
                                                        pagination-id = "newRequest"
                                                        my-repeat-directive = "newRequest">
                                                       
                                                            <td>{{ users.name }}</td>
                                                            <td>{{users.email}}</td>
                                                            <td>{{users.createdOn }}</td>
                                                            <td class="text-center">
                                                                    <button type="button" ng-click="viewDetail(users)" class="btn btn-simple btn-warning btn-icon table-action  edit"  title="View Detail" data-original-title="View Detail"  rel="tooltip"><i class="ti-eye"></i></button>
                                                            </td>
                                                        </tr>

                                                        <tr ng-show="filter_newRequest.length==0">
                                                            <td colspan="4" class="text-center">
                                                                No Data Available
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                            </table>
                                    </div>
                                    

                                    <div class="ng_pageRow">
                                        <div ng-if="filter_newRequest.length>0">
                                         Showing {{from_newRequest}}
                                         to {{  (to_newRequest>=filter_newRequest.length) ? filter_newRequest.length : to_newRequest }} 
                                         of {{filter_newRequest.length}}  entries
                                        </div>
                                       
                                        <dir-pagination-controls
                                        max-size="10"
                                        direction-links="true"
                                        boundary-links="true"
                                        pagination-id = "newRequest">
                                        </dir-pagination-controls>
                                    </div>

                                </div>
                              
                                <div class="tab-pane tabCls" id="blocked">

                                    <div class="row">
                                        <div class="col-sm-2 col-md-2">
                                            <select ng-model="itemsPerPages_blocked" class="form-control ngDT_select"
                                            ng-init="itemsPerPages_blocked =  10"
                                            ng-change="resetPage('blocked')"
                                            ng-options="o as o for o in [5,10,25,100]">
                                            </select>
                                        </div>
                                        <div class="col-sm-7 col-md-8">
                                        </div>
                                        <div class="col-sm-3 col-md-2 pull-right">
                                           <input type="text" ng-model="blockedSearch" class="form-control" placeholder="Search" ng-keyup="onSearch('blocked')">
                                        </div>
                                    </div>
                                    <div class="table-responsive">
                                            <div ng-show="isLoading_blocked">
                                                Loading...
                                            </div>
                                            
                                            <table id="blockedTab" class="table table-no-bordered ngDT" ng-show="!isLoading_blocked" >
                                            <thead>
                                                <tr>
                                                <th ng-click="sort('name','blocked')">
                                                    Name <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyBlocked=='name'" ng-class="{'fa-caret-up':reverseBlocked,'fa-caret-down':!reverseBlocked}"></i>
                                                </th>
                                                <th ng-click="sort('email','blocked')">
                                                    Email <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyBlocked=='email'" ng-class="{'fa-caret-up':reverseBlocked,'fa-caret-down':!reverseBlocked}"></i>
                                                </th>
                                                <th ng-click="sort('createdOn','blocked')">
                                                    Created On <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyBlocked=='createdOn'" ng-class="{'fa-caret-up':reverseBlocked,'fa-caret-down':!reverseBlocked}"></i>
                                                </th>
                                                <th  class="td-actions text-center">Actions</th>
                                                </tr>
                                            </thead>
                                                    <tbody>
                                                        <tr dir-paginate="users in filter_blocked  = (data.blocked | orderBy:sortKeyBlocked:reverseBlocked| filter: blockedSearch)  | itemsPerPage: itemsPerPages_blocked" current-page="currPageBlocked"  
                                                        pagination-id = "blocked"
                                                        my-repeat-directive = "blocked">
                                                            <td>{{ users.name  }}</td>
                                                            <td>{{users.email}}</td>
                                                            <td>{{users.createdOn }}</td>
                                                            <td class="text-center">
                                                               
                                                                    <button type="button"  ng-click="viewDetail(users)" class="btn btn-simple btn-warning btn-icon table-action edit"  title="View Detail" data-original-title="View Detail"  rel="tooltip"><i class="ti-eye"  ></i></button>
                                                                
                                                            </td>
                                                        </tr>

                                                        <tr ng-show="filter_blocked.length==0">
                                                            <td colspan="4" class="text-center">
                                                                No Data Available
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                            </table>
                                    </div>

                                    <div class="ng_pageRow">
                                        <div ng-if="filter_blocked.length>0">
                                         Showing {{from_blocked}}
                                         to {{  (to_blocked>=filter_blocked.length) ? filter_blocked.length : to_blocked }} 
                                         of {{filter_blocked.length}}  entries
                                        </div>
                                        
                                        <dir-pagination-controls
                                        max-size="10"
                                        direction-links="true"
                                        boundary-links="true"
                                        pagination-id = "blocked">
                                        </dir-pagination-controls>
                                    </div>

                                </div>
                             
                                <div class="tab-pane tabCls" id="rejected">

                                    <div class="row">
                                        <div class="col-sm-2 col-md-2">
                                            <select ng-model="itemsPerPages_rejected" class="form-control ngDT_select"
                                            ng-init="itemsPerPages_rejected =  10"
                                            ng-change="resetPage('rejected')"
                                            ng-options="o as o for o in [5,10,25,100]">
                                            </select>
                                        </div>
                                        <div class="col-sm-7 col-md-8">
                                        </div>
                                        <div class="col-sm-3 col-md-2 pull-right">
                                           <input type="text" ng-model="rejectedSearch" class="form-control" placeholder="Search" ng-keyup="onSearch('rejected')">
                                        </div>
                                    </div>
                                    <div class="table-responsive">
                                            <div ng-show="isLoading_rejected">
                                                Loading...
                                            </div>
                                            
                                            <table id="rejectedTab" class="table table-no-bordered ngDT" ng-show="!isLoading_rejected" >
                                            <thead>
                                                <tr>
                                                <th ng-click="sort('name','rejected')">
                                                    Name <i class="fa  ngDT_sortIcon pull-right" ng-show="sortKeyRejected=='name'" ng-class="{'fa-caret-up':reverseRejected,'fa-caret-down':!reverseRejected}"></i>
                                                </th>
                                                <th ng-click="sort('email','rejected')">
                                                    Email <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyRejected=='email'" ng-class="{'fa-caret-up':reverseRejected,'fa-caret-down':!reverseRejected}"></i>
                                                </th>
                                                <th ng-click="sort('createdOn','rejected')">
                                                    Created On <i class="fa ngDT_sortIcon pull-right" ng-show="sortKeyRejected=='createdOn'" ng-class="{'fa-caret-up':reverseRejected,'fa-caret-down':!reverseRejected}"></i>
                                                </th>
                                                <th  class="td-actions text-center">Actions</th>
                                                </tr>
                                            </thead>
                                                    <tbody>
                                                        <tr dir-paginate="users in filter_rejected  = (data.rejected | orderBy:sortKeyRejected:reverseRejected| filter: rejectedSearch)  | itemsPerPage: itemsPerPages_rejected" current-page="currPageRejected"  
                                                        pagination-id = "rejected"
                                                        my-repeat-directive = "rejected">
                                                            <td>{{ users.name }}</td>
                                                            <td>{{users.email}}</td>
                                                            <td>{{users.createdOn }}</td>
                                                            <td class="text-center">
                                                               
                                                                    <button type="button"  ng-click="viewDetail(users)" class="btn btn-simple btn-warning btn-icon table-action edit"  title="View Detail" data-original-title="View Detail"  rel="tooltip"><i class="ti-eye"  ></i></button>
                                                                
                                                            </td>
                                                        </tr>

                                                        <tr ng-show="filter_rejected.length==0">
                                                            <td colspan="4" class="text-center">
                                                                No Data Available
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                            </table>
                                    </div>

                                 
                                    <div class="ng_pageRow">
                                        <div ng-if="filter_rejected.length>0">
                                         Showing {{from_rejected}}
                                         to {{  (to_rejected>=filter_rejected.length) ? filter_rejected.length : to_rejected }} 
                                         of {{filter_rejected.length}}  entries
                                        </div>
                                        
                                        <dir-pagination-controls
                                        max-size="10"
                                        direction-links="true"
                                        boundary-links="true"
                                        pagination-id = "rejected">
                                       </dir-pagination-controls>
                                    </div>

                                </div>
                              </div>
                               
                          
                        </div>
                    </div>
                  </div>
                </div>              
              </div>
          </div>
          <%- include('../parts/footer'); %>
      </div>
  </div>


  <div id="userDetailMdl" class="modal fade" role="dialog">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">User Detail : {{user.shortname}}</h4>
        </div>
        <div class="modal-body">
                    <div class="nav-tabs-navigation" id="detailTab">
                        <div class="nav-tabs-wrapper">
                            <ul id="tabs" class="nav nav-tabs" data-tabs="tabs">
                                <li class="active"><a href="#userinfo" data-toggle="tab">User Info</a></li>
                                <li><a href="#permissions" data-toggle="tab">Permissions</a></li>
                                <li><a href="#history" data-toggle="tab">History</a></li>
                            </ul>
                        </div>
                    </div>
                    <div id="my-tab-content2" class="tab-content">
                        <div class="tab-pane active" id="userinfo">
                           <div class="table-reponsive">
                            <table class="table ">
                                 <tr>
                                     <th width="40%" style="border: none;">
                                        Name
                                     </th>
                                     <td class="text-left" style="border: none;">
                                        {{ user.name }}
                                     </td>
                                 </tr>
                                 <tr>
                                    <th>
                                        Email
                                    </td>
                                    <td class="text-left">
                                      <a href="mailto:{{user.email}}">{{user.email}}</a> 
                                    </td>
                                </tr>
                                <tr>
                                    <th>
                                        Account Status
                                    </th>
                                    <td class="text-left">
                                        <span  ng-show="user.accStatus=='active'" class="label label-success">Active</span>
                                        <span ng-show="user.accStatus=='pending'" class="label label-warning">Pending</span>
                                        <span ng-show="user.accStatus=='blocked'" class="label label-danger">Blocked</span>
                                        <span ng-show="user.accStatus=='rejected'" class="label label-danger">Rejected</span>
                                    </td>
                                </tr>
                                <tr ng-show="user.request">
                                    <th>
                                        Afilliation
                                    </th>
                                    <td class="text-left">
                                        {{user.request.affiliation}}
                                    </td>
                                </tr>
                                <tr ng-show="user.request">
                                    <th>
                                        Department
                                    </th>
                                    <td class="text-left">
                                        {{user.request.department}}
                                    </td>
                                </tr>

                                <tr ng-show="user.request">
                                    <th>
                                        Phone Number
                                    </th>
                                    <td class="text-left">
                                        <a ng-show="user.request.phone.trim()!=''"  href="tel:{{user.request.phone}}">{{user.request.phone}}</a>
                                        <span ng-show="user.request.phone.trim() == ''">Not Available</span>
                                    </td>
                                </tr>

                                <tr ng-show="user.request">
                                    <th>
                                        Personal/Institute web page
                                    </th>
                                    <td class="text-left">
                                        <a 
                                          ng-show="user.request.webpage.trim()!=''" 
                                          target="_blank" href="{{user.request.webpage}}">
                                           {{user.request.webpage}}
                                        </a>
                                        <span ng-show="user.request.webpage.trim()== ''">Not Available</span>
                                    </td>
                                </tr>
                                <tr ng-show="user.request">
                                    <th>
                                        How did you hear about us
                                    </th>
                                    <td class="text-left">
                                        {{user.request.hearAboutUs}}
                                    </td>
                                </tr>
                                <tr ng-show="user.request">
                                    <th>
                                        Courses you taught or you plan to teach
                                    </th>
                                    <td class="text-left">
                                        {{user.request.courseTaught}}
                                    </td>
                                </tr>

                                <tr>
                                    <th>
                                        Note
                                    </th>
                                    <td class="text-left">
                                        <div ng-show="isNoteEdit">
                                            <textarea class="form-control" ng-model="user.note1" maxlength="500" placeholder="Enter note about author" rows="5"></textarea>
                                            <br>
                                            

                                                <button type="button" ng-click="saveNote(user)" class="btn btn-success btn-sm" rel="tooltip"  title="Save">
                                                    <i class="fa fa-floppy-o"></i>
                                                </button>
                                                <button type="button" ng-click="cancelEditNote()" class="btn btn-danger btn-sm" rel="tooltip" title="Cancel">
                                                    <i class="fa fa-times"></i>
                                                </button>
                                            
                                        </div>
                                       
                                        
                                        <p ng-show="!isNoteEdit">
                                            {{ user.note }}
                                            <button type="button" ng-click="editNote(user)" class="btn btn-primary btn-sm" rel="tooltip" title="Edit">
                                                <i class="fa fa-pencil-square-o"></i>
                                            </button>
                                        </p>
                                        
                                        
                                        
                                    </td>
                                </tr>
                            </table>
                           </div>
                            <hr>
                            <div class="text-right">
                              <button type="button" ng-click="setStatus('blocked',user)" ng-show="user.accStatus=='active'" class="btn  btn-danger">Block</button>
                              <button type="button" ng-show="user.accStatus=='blocked' || user.accStatus=='rejected' || user.accStatus=='pending'" class="btn  btn-success" ng-click="setStatus('active',user)">
                                  {{ activeBtnText(user.accStatus) }}
                              </button>
                              <button type="button" ng-click="setStatus('rejected',user)" ng-show="user.accStatus=='pending'" ng-disabled="disRejBtn"  class="btn  btn-danger">
                                  Reject</button>
                            </div>
                           
                        </div>
                        <div class="tab-pane" id="permissions">
                            <div class="checkbox">
                                <input id="checkbox1" type="checkbox" ng-model="user.permission.quiz" ng-checked="user.auth.access.includes('quiz')">
                                <label for="checkbox1">
                                    <span class="standOut">Quiz</span> (Create and manage sectional and  live quizzes) 
                                </label>
                            </div>

                            <div class="checkbox">
                                <input id="checkbox2" type="checkbox" ng-model="user.permission.publicRead" ng-checked="user.auth.access.includes('publicRead')" ng-disabled= "user.permission.publicWrite" >
                                <label for="checkbox2">
                                    <span class="standOut"> Public repository read only </span>   (Allowing user to view content  in the public repository and raise tickets)

                                </label>
                            </div>

                            <div class="checkbox">
                                <input id="checkbox3" type="checkbox" ng-change="publicWriteChange()"  ng-model="user.permission.publicWrite" ng-checked="user.auth.access.includes('publicWrite')">
                                <label for="checkbox3">
                                    <span class="standOut"> Public repository write </span> (allowing user to add content in the public repository )
                                </label>
                            </div>

                            <div class="checkbox">
                                <input id="checkbox4" type="checkbox" ng-model="user.permission.workbook" ng-checked="user.auth.access.includes('workbook')">
                                <label for="checkbox4">
                                    <span class="standOut"> Workbook </span> (Create and  publish workbooks)
                                </label>
                            </div>

                            <div class="checkbox">
                                <input id="checkbox5" type="checkbox" ng-model="user.permission.admin" ng-checked="user.auth.access.includes('admin')">
                                <label for="checkbox5" class="text-danger">
                                    <span class="standOut"> Admin</span>  (Manage administrative tasks like author management )
                                </label>
                            </div>

                            <hr>
                            <div class="text-right">
                              <button type="button"  class="btn  btn-primary" ng-click="changePermission(user)">Change Permission</button>
                            </div>

                        </div>
                        <div class="tab-pane" id="history">
                            <p ng-show="user.history.length==0" class="text-center">Not Available</p>
                            <div class="table-reponsive" ng-show="user.history.length>0">
                                <table class="table">
                                    <tr>
                                        <th  style="border: none;">
                                        Action
                                        </th>
                                        <th style="border: none;">
                                            Date
                                        </th>
                                    </tr>
                                    <tr ng-repeat = "hist in user.history | orderBy: '-when'">
                                        <td class="text-left">{{hist.msg}}</td>
                                        <td class="text-left">{{hist.when | localDate}}</td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
              
        </div>
      </div>
    </div>
  </div>

</body>
