<!-- <div class="tab-pane quesYScroll" id="parameters"> -->
<div class="tab-pane " id="parameters">
    <div class="form-group row ">
        <div class="col-xs-10 col-xs-offset-1 col-md-10 col-md-offset-1 inMiddle">
                <label class='control-label'>Parameters
                        <button type="button" data-toggle="popover"  class="btn btn-info btn-simple btn-xs" data-content="<%-tooltip.tooltipQueParameter01%>">
                         <i class="ti-help-alt fa-lg"></i>
                        </button>
                        </label>
                        <div id='allParams'>
                                <% 
                                    var parmdivs = 0 ;
                                    if(typeof data.parameters == 'object' && data.parameters){
                                        // answers are there 
                                        parmdivs = data.parameters.length;
                                    }else{
                                        parmdivs = 0;
                                    }
                                %>
                                 <%for(var j=0; j< parmdivs;j++){%>
                                    <div class='col-lg-12 ' id="paramdiv_old-<%-j%>" >
                                    <div class='card inputcard1' >
                                        <div class='card-content'>
                                            <h6>Name</h6>
                                            <div>
                                                <input type="text" name="param_name_old-<%-j%>" value="<%if(typeof data.parameters == 'object' && data.parameters){%><%- data.parameters[j].param %><%}else{%><%}%>" class="form-control closeBracket" id="param_name_old-<%-j%>" >
                                            </div>
                                            <br>
                                            <h6>Value</h6>
                                            <textarea  name="param_value_old" class="form-control" id="param_value_old"><%if(typeof data.parameters == 'object' && data.parameters){%><%- data.parameters[j].values %><%}else{%><%}%></textarea>
                                        </div>
                                        <div class='card-footer footer1'>
                                            <hr>
                                          
                                            <div class="footer-title"><span class="text-danger"> &nbsp;</span></div>
                                            <div class="pull-right">
                                                <span class="text-muted"> &nbsp; </span> 
                                                    <button type='button' rel='tooltip' title='' class='btn btn-info' data-paraType='old' data-paraId='<%-j%>' data-original-title='Open group parameter editor' id='editGrpParam'>
                                                        <i class='ti-pencil'></i>
                                                    </button>
                                                    <button type="button" rel="tooltip" title="" class="btn btn-danger" data-original-title="Delete" 
                                                     id='deleteParam'>
                                                    <i class="ti-trash"></i>
                                                    </button>                 
                                            </div>
                                        </div>
                                    </div>
                                    </div>
                                <%}%>
                
                            </div>
                            <div align="right">            
                                    <input class='btn btn-default' id="addNewParam" type="button" value="Parameter +" />    
                            </div>

        </div>
    </div>

</div>



<!-- open parameter editor modal -->
  <div class="modal  fade" tabindex="-1" role="dialog" id="editParmMdl" data-backdrop="static">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title">Parameter editor</h4>
        </div>
        <div class="modal-body">
            <p>Note that the first row must contain the parameter name. If the table has only two rows and one column,it will be considered as a normal parameter. </p>
         <table id="parmTbl"></table>
         <span id="parmTblErr" class="text-danger"></span>
        </div>
        <div class="modal-footer">
            <input type="hidden" id="selectedParam" data-paraType="" data-paraId=" ">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" id="saveParm" class="btn btn-primary">Save changes</button>
        </div>
      </div>
    </div>
  </div>