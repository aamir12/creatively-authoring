/* form */
#newlogin .lgFrm {
    width: 270px;
    margin: 45px auto;
  }
#newlogin .lgFrm h1 {
    font-size: 3em;
    font-weight: 300;
    text-align: center;
    color: #2196F3;
  }

#newlogin .lgFrm h5 {
    text-align: center;
    text-transform: uppercase;
    color: #c6c6c6;
}

#newlogin .lgFrm hr.sep {
    background: #2196F3;
    box-shadow: none;
    border: none;
    height: 2px;
    width: 25%;
    margin: 0px auto 45px auto;
}


  
#newlogin .lgFrm .group {
    position: relative;
    margin: 40px 0;
}
  

  
#newlogin  .lgFrm input
{
    background: none;
    color: #3b3b44;
    font-size: 18px;
    padding: 10px 10px 10px 5px;
    display: block;
    width: 270px;
    border: none;
    border-radius: 0;
    border-bottom: 1px solid #c6c6c6;
}


#newlogin .lgFrm  input:focus
{
    outline: none;
}
  
#newlogin .lgFrm input:focus ~ label,#newlogin .lgFrm input:valid ~ label
{
    top: -14px;
    font-size: 12px;
    color: #868686;
}
#newlogin .lgFrm  input:focus ~ .bar:before
{
    width: 270px;
}
  

  
#newlogin .lgFrm label {
    color: #c6c6c6;
    font-size: 18px;
    font-weight: normal;
    position: absolute;
    pointer-events: none;
    left: 5px;
    top: 10px;
    transition: 300ms ease all;
}
  
#newlogin .lgFrm .bar {
    position: relative;
    display: block;
    width: 270px;
}

#newlogin .lgFrm .bar:before {
    content: '';
    height: 2px;
    width: 0;
    bottom: 0px;
    position: absolute;
    background: #bababb;
    transition: 300ms ease all;
    left: 0%;
}

#newlogin  .viewPas{
    position: absolute;
    right: 0px;
    bottom:18px;
    font-size: 18px;
}

#newlogin .imgDiv img{
    margin:auto;
    /* margin-bottom:0px;
    bottom:0 */
}

#imglogo{
    display: none;
}

.bodyCss{
